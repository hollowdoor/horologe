  },
  "keywords": [
    "timer",
    "time"
  ],
  "author": "Quentin Engles <hollowdoor99@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "more-events": "^1.2.1"
  },
  "tonicExampleFilename": "example.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hollowdoor/horologe.git"
  },
  "bugs": {
    "url": "https://github.com/hollowdoor/horologe/issues"
  },
  "homepage": "https://github.com/hollowdoor/horologe#readme",
  "ebam": {
    "input": "src/index.js",
